<h2 class="text-center mt-4">All trips</h2>

<section class="experiences-index p-5">
  <div class="row p-3">
    {{#each travels}}
    <div class="col-xs-3 col-md-3 text-center mb-4">
      <a href="/travel/{{this._id}}">
        <img src={{this.photos.[0]}} class="img-fluid rounded mb-1">
        <h6>{{this.title}}</h6>
      </a>
    </div>
    {{/each}}
  </div>
</section>